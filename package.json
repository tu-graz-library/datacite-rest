    "@rollup/plugin-node-resolve": "^7.1.3",
    "crypto-js": "^4.0.0",
    "json": "^10.0.0",
    "lodash": "^4.17.15",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.10.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-url": "^3.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.0"
  }
